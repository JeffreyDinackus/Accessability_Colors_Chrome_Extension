{
  "manifest_version": 2,
  "name": "Accessibility Colors",
  "version": "0.0.1",
  "description": "Color schemes for the colorblind",
  "permissions": [
    "activeTab",
    "cookies"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}