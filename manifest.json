{
  "manifest_version": 3,
  "name": "Accessibility Colors",
  "version": "0.0.1",
  "description": "Color schemes for the colorblind",
  "permissions": [
    "storage"
  ],
  "icons": {
    "128": "icon-128x128.png",
    "48": "icon-48x48.png",
    "16": "icon-16x16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": {}
}